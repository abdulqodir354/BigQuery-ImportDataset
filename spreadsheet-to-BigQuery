-- QUIZ no.1 (menampilkan semua data di tabel superstore)
SELECT *
 FROM `dqlab-bootcampda-qodir.superstore.transaction` LIMIT 1000
;

-- QUIZ no.2
-- menampilkan informasi: 
-- Order_ID,Ship_Date,Sales,Discount,Profit
 SELECT
  Order_ID,
  Ship_Date,
  Sales,
  Discount,
  Profit,
 FROM `superstore.transaction`;

-- QUIZ no.3
-- Tampilkan seluruh kombinasi kategori dan sub kategori yang sudah pernah terjual
SELECT DISTINCT Category, Sub_Category
FROM `superstore.transaction`
WHERE Quantity > 1;

